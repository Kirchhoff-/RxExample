<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kirchhoff.rxexample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".ui.FirstActivity" />

        <activity android:name=".ui.MapOperatorActivity" />

        <activity android:name=".ui.ZipOperatorActivity" />

        <activity android:name=".ui.DisposableActivity" />

        <activity android:name=".ui.TakeOperatorActivity" />

        <activity android:name=".ui.TimerActivity" />

        <activity android:name=".ui.IntervalActivity" />

        <activity android:name=".ui.SingleObserverActivity" />

        <activity android:name=".ui.CompletableObserverActivity" />

    </application>

</manifest>