<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.kirchhoff.rxexample.MainActivity">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <Button
            android:id="@+id/firstExample"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="First Example" />


        <Button
            android:id="@+id/mapOperator"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Map operator" />

        <Button
            android:id="@+id/zipOperator"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Zip operator" />

        <Button
            android:id="@+id/disposable"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Disposable" />

        <Button
            android:id="@+id/takeOperator"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Take operator" />


        <Button
            android:id="@+id/timer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Timer" />

        <Button
            android:id="@+id/interval"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Interval" />

        <Button
            android:id="@+id/singleObserver"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Single Observer" />


        <Button
            android:id="@+id/completableObserver"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Completable Observer" />

        <Button
            android:id="@+id/flowable"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Flowable" />

        <Button
            android:id="@+id/reduceOperator"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Reduce Operator" />

        <Button
            android:id="@+id/bufferOperator"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Buffer operator" />

        <Button
            android:id="@+id/filterOperator"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Filter Operator" />

        <Button
            android:id="@+id/skipOperator"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Skip Operator" />

        <Button
            android:id="@+id/scanOperator"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Scan operator" />

        <Button
            android:id="@+id/replayOperator"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Replay Operator" />

        <Button
            android:id="@+id/concatOperator"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Concat Operator" />

        <Button
            android:id="@+id/mergeOperator"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Merge Operator" />

        <Button
            android:id="@+id/deferObservable"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Defer Observable" />

        <Button
            android:id="@+id/distinctOperator"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Distinct Operator" />


        <Button
            android:id="@+id/lastOperator"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Last Operator" />

        <Button
            android:id="@+id/replaySubject"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Replay Subject" />

        <Button
            android:id="@+id/publishSubject"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Publish Subject" />

        <Button
            android:id="@+id/behaviorSubject"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Behavior Subject" />

        <Button
            android:id="@+id/asyncSubject"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Async Subject" />

        <Button
            android:id="@+id/throttleFirst"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Throttle First" />

        <Button
            android:id="@+id/throttleLast"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Throttle Last" />

        <Button
            android:id="@+id/debounceOperator"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Debounce operator" />

        <Button
            android:id="@+id/windowOperator"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Window operator" />

    </LinearLayout>

</ScrollView>
